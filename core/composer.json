{
    "name": "services/core",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^12.1",
        "spiral/code-style": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Zinc\\Core\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "require": {
        "psr/log": "^3.0",
        "symfony/uid": "^7.2",
        "symfony/var-dumper": "^7.2",
        "symfony/messenger": "^7.2"
    },
    "scripts": {
        "cs:diff": "php-cs-fixer fix --dry-run -v --diff",
        "cs:fix": "php-cs-fixer fix -v"
    }
}
